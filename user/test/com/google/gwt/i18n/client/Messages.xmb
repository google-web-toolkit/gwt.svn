<!-- I generate a class named Messages -->
<messagebundle class="com.google.i18n.example.holamundo.Messages"> 


  <!-- A message named WorldMessage (mangled to WORLD_MESSAGE in Java),
       with hint to translators "Greetings to the world"
       and content "Hello, world!" 
    -->
  <msg name="WorldMessage" desc="Greetings to the world">
    Hello, world!
  </msg>

</messagebundle>
